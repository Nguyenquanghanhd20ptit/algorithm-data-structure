#include<bits/stdc++.h>
using namespace std;
int n,a[100],cot[100]={0},d1[100]={0},d2[100]={0};
int x[100][100];
void inkq(){
	memset(x,0,sizeof(x));
	for(int i=1;i<=n;i++){
		x[i][a[i]]=1;
	}
	for(int i=1;i<=n;i++){
		for(int j=1;j<=n;j++){
			cout<<x[i][j]<<" ";
		}
		cout<<endl;
	}
	cout<<endl;
}
void Try(int i){
	for(int j=1;j<=n;j++){
		if(cot[j]==0 && d1[i-j+n]==0 && d2[i+j-1]==0){
			a[i]=j;
			cot[j]=d1[i-j+n]=d2[i+j-1]=1;
			if(i==n){
				inkq();
			}
			else Try(i+1);
			cot[j]=d1[i-j+n]=d2[i+j-1]=0;
		}
	}
}
int main(){
	int t; cin>>t;
	while(t--){
		cin>>n;
		Try(1);
	}
}
